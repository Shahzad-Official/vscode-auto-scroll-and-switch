{
  "name": "vscode-auto-scroll-and-switch",
  "displayName": "Auto Scroll And Switch",
  "description": "Automatically scroll through files and switch between tabs for monitoring and presentations",
  "version": "0.0.5",
  "publisher": "local",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shahzad-Official/vscode-auto-scroll-and-switch.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autoScroll.start",
    "onCommand:autoScroll.stop"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoScroll.start",
        "title": "Auto Scroll: Start"
      },
      {
        "command": "autoScroll.stop",
        "title": "Auto Scroll: Stop"
      },
      {
        "command": "autoScroll.checkUpdates",
        "title": "Auto Scroll: Check for Updates"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.0.0"
  }
}
